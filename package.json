{
  "name": "@hitomihiumi/lazy-canvas",
  "version": "1.5.1",
  "description": "Simple module for using canvas in lazy method",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "tsc ./scripts/levelup.ts && node --trace-warnings ./scripts/levelup.js",
    "test:rank": "tsc ./scripts/rank.ts && node --trace-warnings ./scripts/rank.js",
    "test:error": "tsc ./scripts/error.ts && node --trace-warnings ./scripts/error.js",
    "test:text": "tsc ./scripts/text.ts && node --trace-warnings ./scripts/text.js",
    "test:link": "tsc ./scripts/link.ts && node --trace-warnings ./scripts/link.js",
    "test:pattern": "tsc ./scripts/pattern.ts && node --trace-warnings ./scripts/pattern.js",
    "test:path2d": "tsc ./scripts/path2d.ts && node --trace-warnings ./scripts/path2d.js",
    "test:svg": "tsc ./scripts/svg.ts && node --trace-warnings ./scripts/svg.js",
    "test:transform": "tsc ./scripts/transform.ts && node --trace-warnings ./scripts/transform.js",
    "docs": "node docgen.js",
    "docs:example": "tsc ./scripts/docs.ts && node ./scripts/docs.js",
    "ava": "tsc ./test/test.ts && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitomihiumi/lazy-canvas-ts.git"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "keywords": [
    "canvas",
    "lazycanvas",
    "napi-rs/canvas",
    "jimp",
    "svgson",
    "lazy",
    "simple",
    "easy"
  ],
  "author": "hitomihiumi",
  "license": "MIT",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.55",
    "jimp": "^0.22.12",
    "path": "^0.12.7",
    "svgson": "^5.3.1"
  },
  "devDependencies": {
    "@hitomihiumi/colors.ts": "^1.0.3",
    "@hitomihiumi/micro-docgen": "^0.2.1",
    "@neplex/vectorizer": "^0.0.4",
    "@types/jimp": "^0.2.28",
    "ava": "^6.1.3",
    "globals": "^15.11.0",
    "tslib": "^2.6.3",
    "typedoc": "^0.26.4",
    "typedoc-material-theme": "^1.1.0",
    "typedoc-plugin-extras": "^3.1.0",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript": "^5.4.5"
  },
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/hitomihiumi/lazy-canvas-ts/issues"
  },
  "homepage": "https://github.com/hitomihiumi/lazy-canvas-ts#readme"
}
